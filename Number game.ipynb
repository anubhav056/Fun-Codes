    "import random\n",
    "n=int(input('No of die to roll:'))\n",
    "s=input('Roll dice? Yes/No:')\n",
    "num=[1,2,3,4,5,6]\n",
    "for i in range (0,n):\n",
    "    if (s=='Yes'):\n",
    "        random_num=random.choice(num)\n",
    "        print(random_num)\n",
    "    else:\n",
    "        pass"
   ]
